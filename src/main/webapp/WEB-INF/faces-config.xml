<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <locale-config>
            <default-locale >zh</default-locale>
        </locale-config>
        <action-listener>
            org.primefaces.application.DialogActionListener
        </action-listener>
        <navigation-handler>
            org.primefaces.application.DialogNavigationHandler
        </navigation-handler>
        <view-handler>
            org.primefaces.application.DialogViewHandler
        </view-handler>
                
        <resource-bundle>
            <base-name>/org/eman/bundle</base-name>
            <var>bundle</var>
        </resource-bundle>     

        <resource-bundle>
            <base-name>/org/peasant/util/i18n</base-name>
            <var>i18nBundle</var>
        </resource-bundle>        
        <resource-bundle>
            <base-name>/org/peasant/security_i18n</base-name>
            <var>security_i18n</var>
        </resource-bundle>

        <resource-bundle>
            <base-name>/org/eman/asist_i18n</base-name>
            <var>asist_i18n</var>
        </resource-bundle>
        
        <resource-bundle>
            <base-name>/org/eman/basic_i18n</base-name>
            <var>basic_i18n</var>
        </resource-bundle>


        <resource-bundle>
            <base-name>/org/eman/gmsys_i18n</base-name>
            <var>gmsys_i18n</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/org/peasant/security_i18n</base-name>
            <var>security_i18n</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/org/eman/asist_i18n</base-name>
            <var>asist_i18n</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/peasant/i18n</base-name>
            <var>i18n</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/org/peasant/ORGStructure_i18n</base-name>
            <var>oRGStructure_i18n</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/org/peasant/i18n_basic</base-name>
            <var>i18n_basic</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/org/peasant/i18n_basic</base-name>
            <var>i18n_basic</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/org/peasant/i18n_basic</base-name>
            <var>i18n_basic</var>
        </resource-bundle>
    </application>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/security/login.jsf</to-view-id>
        </navigation-case>    
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>setCandidatevalues</from-outcome>
            <to-view-id>/asist/asistCandidateValueTree/customize</to-view-id>
        </navigation-case>    
    </navigation-rule>
    
    
    
    <navigation-rule>
        <description>选择数据</description>
        <navigation-case>
            <from-outcome>selectEquipmentModel</from-outcome>
            <to-view-id>/basic/equipmentModel/select</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>selectRoomspot</from-outcome>
            <to-view-id>/basic/roomspot/select</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>selectStation</from-outcome>
            <to-view-id>/basic/station/select</to-view-id>
        </navigation-case>
    </navigation-rule>
          
    
    
    
</faces-config>
